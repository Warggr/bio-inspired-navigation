      <inertial>
        <mass value="1"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>


        <joint name="box_4_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <parent link="planeLink"/>
      <child link="box_4"/>
    </joint>

    <link name="box_4">
